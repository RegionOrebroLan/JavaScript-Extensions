{
	"compilerOptions": {
		"declaration": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"noImplicitAny": false,
		"outDir": "wwwroot/Scripts/",
		"sourceMap": false,
		"strict": true,
		"strictNullChecks": true,
		"target": "es5"
	},
	"exclude": [
		"node_modules",
		"wwwroot"
	],
	"include": [
		"Scripts/**/*.ts"
	]
}