{
	"compilerOptions": {
		"declaration": true,
		"downlevelIteration": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"noImplicitAny": false,
		"outDir": "lib",
		"sourceMap": true,
		"strict": true,
		"strictNullChecks": true,
		"target": "es5"
	},
	"exclude": [
		"dist",
		"lib",
		"node_modules",
		"Samples",
		"test"
	],
	"include": [
		"src"
	]
}